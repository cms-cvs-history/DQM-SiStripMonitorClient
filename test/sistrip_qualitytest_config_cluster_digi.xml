<TESTSCONFIGURATION> 
<QTEST name="XrangeWithin:0-3.0" activate="true"> 
     <TYPE>ContentsXRange</TYPE> 
     <PARAM name="error">0.4</PARAM> 
     <PARAM name="warning">0.6</PARAM> 
     <PARAM name="xmin">0</PARAM> 
     <PARAM name="xmax">3.0</PARAM> 
</QTEST>
<QTEST name="XrangeWithin:0-5.0" activate="true"> 
     <TYPE>ContentsXRange</TYPE> 
     <PARAM name="error">0.4</PARAM> 
     <PARAM name="warning">0.6</PARAM> 
     <PARAM name="xmin">0</PARAM> 
     <PARAM name="xmax">5.0</PARAM> 
</QTEST>
<QTEST name="MeanWithinExpected:RMS" activate="true"> 
     <TYPE>MeanWithinExpected</TYPE> 
     <PARAM name="error">0.05</PARAM> 
     <PARAM name="warning">0.5</PARAM> 
     <PARAM name="mean">1.0</PARAM> 
     <PARAM name="useRMS">1</PARAM> 
     <PARAM name="useSigma">0</PARAM>
     <PARAM name="useRange">0</PARAM> 
     <PARAM name="xmin">0</PARAM> 
     <PARAM name="xmax">0</PARAM> 
</QTEST>

<LINK name="*NumberOfDigis__det__*"> 
  <TestName activate="true">MeanWithinExpected:RMS</TestName> 
</LINK>
<LINK name="*NumberOfClusters__det__*"> 
  <TestName activate="true">XrangeWithin:0-3.0</TestName> 
</LINK>
<LINK name="*ClusterWidth__det__*"> 
  <TestName activate="true">XrangeWithin:0-5.0</TestName> 
</LINK>
</TESTCONFIGURATION>
